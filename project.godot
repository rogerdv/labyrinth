; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="The Labyrinth of Knowledge"
config/description="The Labyrinth remake"
run/main_scene="res://Scenes/main_menu.tscn"
boot_splash/image="res://Images/logo-splash.png"
config/icon="res://Images/logo ico 2.png"

[autoload]

GameInstance="*res://Scripts/GameInstance.gd"

[display]

window/size/width=854
window/size/height=480
window/stretch/mode="2d"
window/stretch/aspect="keep_width"

[editor_plugins]

enabled=PoolStringArray( "gut" )

[input]

ui_select={
