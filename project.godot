config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="The Labyrinth of Knowledge"
config/description="The Labyrinth remake"
run/main_scene="res://Scenes/main_menu.tscn"
config/icon="res://Images/logo ico 2.png"

[autoload]

GameInstance="*res://Scripts/GameInstance.gd"

[display]

window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "gut" )

[input_devices]

pointing/emulate_touch_from_mouse=true

[locale]

translations=PoolStringArray( "res://UI/text.en.translation", "res://UI/text.es.translation" )

[rendering]

quality/driver/fallback_to_gles2=true
vram_compression/import_etc=true
environment/default_environment="res://default_env.tres"
